<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_plan__complete"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="350dp"
    android:layout_height="wrap_content"
    android:clickable="false"
    tools:context="com.hwi.health.Activitys.Profile.First.PlanActivity_Complete">

  <!--  <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:layout_above="@+id/bottom"
        app:cardElevation="@dimen/dp5"
        app:cardUseCompatPadding="true">
-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/bottom"
            android:orientation="vertical"
            android:background="#51000000"
            android:padding="10dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Thanks for completing Advance Profile, we have generated"
                android:textColor="@color/app_org"
                android:textSize="18dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="30dp"
                android:gravity="center"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/health_lin"
                android:visibility="gone"
                android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp15"
                android:textColor="@color/white"
                android:text="Health Profile"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/app_org"></View>

        </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:id="@+id/heart_lin"
                android:visibility="gone"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/dp15"
                    android:textColor="@color/white"
                    android:text="Heart Risk Profile"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/app_org"></View>

            </LinearLayout>



            <Button
                android:id="@+id/view_plan"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text="view profile"
                android:textColor="@color/white"
                android:paddingLeft="@dimen/dp5"
                android:paddingRight="@dimen/dp5"
                android:background="@color/app_org"
                android:gravity="center"
                android:textSize="12dp"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="60dp"/>
        </LinearLayout>

    <!--</android.support.v7.widget.CardView>-->


</RelativeLayout>
